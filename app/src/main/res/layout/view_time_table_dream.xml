<?xml version="1.0" encoding="utf-8"?>

<android.support.v7.widget.CardView
    android:id="@+id/view_card_tel_view"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    card_view:cardCornerRadius="3dp"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <!--竹富-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.dream.DreamTimeTableTaketomiView
            android:id="@+id/view_dream_time_table_taketomi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            />

        <!--小浜-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.dream.DreamTimeTableKohamaView
            android:id="@+id/view_dream_time_table_kohama"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            />

        <!--大原-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.dream.DreamTimeTableOoharaView
            android:id="@+id/view_dream_time_table_oohara"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            />

        <!--黒島-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.dream.DreamTimeTableKuroshimaView
            android:id="@+id/view_dream_time_table_kurhoshima"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            />

        <!--上原/鳩間-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.dream.DreamTimeTableUeharaView
            android:id="@+id/view_dream_time_table_uehara"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

    </LinearLayout>
</android.support.v7.widget.CardView>