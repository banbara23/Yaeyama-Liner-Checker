<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    android:layout_marginEnd="@dimen/status_detail_view_outer_space"
    android:layout_marginStart="@dimen/status_detail_view_outer_space"
    card_view:cardCornerRadius="3dp"
    >

    <LinearLayout
        android:id="@+id/view_timetable_root_annei"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <!--竹富-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableTaketomiView
            android:id="@+id/view_time_table_annei_taketomi"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--上原-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableUeharaView
            android:id="@+id/view_time_table_annei_uehara"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--大原-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableOoharaView
            android:id="@+id/view_time_table_annei_oohara"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--小浜-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableKohamaView
            android:id="@+id/view_time_table_annei_kohama"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--黒島-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableKuroshimaView
            android:id="@+id/view_time_table_annei_kurhoshima"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--鳩間-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableHatomaView
            android:id="@+id/view_time_table_annei_hatoma"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />

        <!--波照間-->
        <com.ikmr.banbara23.yaeyama_liner_checker.timetable.annei.AnneiTimeTableHaterumaView
            android:id="@+id/view_time_table_annei_hateruma"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            />
    </LinearLayout>
</android.support.v7.widget.CardView>